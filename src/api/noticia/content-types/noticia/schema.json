{
  "kind": "collectionType",
  "collectionName": "noticias",
  "info": {
    "singularName": "noticia",
    "pluralName": "noticias",
    "displayName": "Noticias",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "subtitle": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "noticias"
    },
    "publicacion": {
      "type": "date",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
